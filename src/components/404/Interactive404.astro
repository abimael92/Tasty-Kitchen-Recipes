<script>
	// Set current URL and timestamp
	document.getElementById('current-url').textContent = window.location.href;
	document.getElementById('error-timestamp').textContent =
		new Date().toLocaleString();

	// Animate digits
	document.querySelectorAll('.digit').forEach((digit, index) => {
		setTimeout(() => {
			digit.style.transform = 'translateY(0)';
			digit.style.opacity = '1';
		}, index * 200);
	});

	// Floating icon animation
	const icon = document.querySelector('.floating-icon');
	let angle = 0;
	setInterval(() => {
		angle += 0.5;
		icon.style.transform = `translate(-50%, -50%) rotate(${angle}deg)`;
	}, 50);

	// Particle background
	function createParticles() {
		const particles = document.querySelector('.particles-background');
		for (let i = 0; i < 20; i++) {
			const particle = document.createElement('div');
			particle.className = 'particle';
			particle.style.left = `${Math.random() * 100}%`;
			particle.style.top = `${Math.random() * 100}%`;
			particle.style.animationDelay = `${Math.random() * 5}s`;
			particles.appendChild(particle);
		}
	}

	// Search functionality
	document.querySelector('.search-form').addEventListener('submit', (e) => {
		e.preventDefault();
		const query = document.getElementById('site-search').value;
		if (query.trim()) {
			window.location.href = `/search?q=${encodeURIComponent(query)}`;
		}
	});

	// Error reporting
	window.reportError = function () {
		const url = window.location.href;
		const referrer = document.referrer;

		// In a real app, you would send this to your backend
		console.log('Error reported:', { url, referrer, timestamp: new Date() });
		alert('Thank you for reporting! This helps us improve the site.');

		// Optional: Send to analytics
		if (typeof gtag !== 'undefined') {
			gtag('event', '404_report', {
				event_category: 'errors',
				event_label: url,
			});
		}
	};

	// Initialize on load
	document.addEventListener('DOMContentLoaded', () => {
		createParticles();
	});
</script>

